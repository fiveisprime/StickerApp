  "version": "0.2.0",
  "compounds": [
    {
      "name": "Full Stack App",
      "configurations": ["Launch Server", "Launch Edge"]
    }
  ],
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "program": "${workspaceFolder}/server/index.js"
    },
    {
      "type": "edge",
      "request": "launch",
      "name": "Launch Edge",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
